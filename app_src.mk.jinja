{%- for dir in include_directories -%}
INCDIR += -I {{ dir }}
{% endfor -%}

{% for file in target.arm + source + target.stm32f4 -%}
{% if file.endswith('.c') %}CSRC += {{ file }}
{% elif file.endswith('.s') %}ASMSRC += {{ file }}
{% elif file.endswith('.cpp') %}CPPSRC += {{ file }}
{% endif %}{% endfor %}
